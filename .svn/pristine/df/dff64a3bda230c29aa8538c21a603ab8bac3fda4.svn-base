*{margin:0;padding:0;-webkit-tap-highlight-color: transparent;}
body{font-size:0.28rem;font-family: -apple-system, BlinkMacSystemFont, "PingFang SC","Helvetica Neue",STHeiti,"Microsoft Yahei",Tahoma,Simsun,sans-serif;}
/* tou */
.public_header_container{}
.public_header_nav{
    box-sizing: border-box;
    position: relative;
    border: 1px solid #f6f6f6;
    background-color:#ffffff;
}
.public_header_nav .public_header_home{
    display: block;
    width:0.88rem;
    height:0.88rem;
    background: url('../images/public/top_icon_homepage.png') center no-repeat;
    background-size:0.48rem;
    position: absolute;
    left:0;top:0;
}
.public_header_nav .public_header_title{
    text-align: center;
    font-family: PingFang-SC-Medium;
    font-size: 0.4rem;
    font-weight: 400;
    color: #333333;
    padding: 0 1rem;
    height:0.88rem;
    line-height: 0.88rem;
}
.public_header_nav .public_header_mytest{
    font-size:0.28rem;
    text-decoration: none;
    height: 0.5rem;
    line-height: 0.5rem;
    border: 1px solid #DB4852;
    border-radius: 0.1rem;
    padding: 0 0.1rem;
    color: #DB4852;
    margin-right:0.2rem;
    position: absolute;
    top:50%;
    right:0;
    margin-top:-0.25rem;
}
.public_banner{font-size:0;}
/* jiao */
.public_fonter_container{padding-bottom: 1rem;background-color:transparent;overflow: hidden;}
.public_fonter_content{text-align: center;margin-top:0.4rem;}
.public_fonter_content p{font-size:0.28rem;color:#333333;line-height:1.5;}
.public_fonter_content p:nth-last-child(1){font-size:0.18rem;color:#6c6c6c;padding:0.2rem 0;}
