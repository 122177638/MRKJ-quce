body {
    font-family: Arial,Helvetica,sans-serif;
    text-align: center;  
    -webkit-text-size-adjust: none;
}
body, h1, h2, h3, p, div, ol, ul, input, button {
    margin: 0;
    padding: 0;
}
body, #page {
    height: inherit;
}
.main-wrap {
    width: 100%; 
    padding-top:0.9rem;
}
.head-wrap{
    margin: 0.3rem auto 0.6rem;
}
.head-logo{
    width: 60%;
    margin:0 auto;
    display:block;
}
.head-title-img{
    width: 40%;
    display:block;
    margin:0.2rem auto;
    
}
.submit-wrap{
    margin-left: 0.2rem;
    margin-right: 0.2rem;
}
.submit-wrap div{
    padding:0.1rem;
    -webkit-appearance: none;
}				

#submit-btn{
    margin: 0 auto;
    margin-top: 0.2rem;
    height:1rem;
    width: 60%;
    color: #fff;
    border: 0;
    font-size: 0.48rem;
    background-color: #F56F70;
    border-radius:0.2rem;
    line-height: 1rem;
}
.select-title{
    overflow: hidden;
}
.gt-wrap,.bt-warp,.null-space{
    display: inline-block;
    float: left;
    width: 40%;
    text-align: center;
}
.null-space{
    width: 20%;
    height: 1px;
}
.title-img{
    width: 60%;
}
.help-tip{
    color:#9d9d9d;
    font-size: 0.24rem;
}
.select-wrap{
    display: -webkit-box;
    -webkit-box-align: center;
    -webkit-box-pack: center;
    display: flex;
    justify-content: center;
    align-items: center;
}

.sel-boy{
    position: relative;
    -webkit-box-flex:2;
    flex: 2;
    width: 40%;
}
.sel-tip{
    position: relative;
    -webkit-box-flex:1;
    flex: 1;  
    width: 20%;   
    height: 0.4rem;
    line-height: 0.4rem;    	
}
.sel-girl{
    position: relative;
    -webkit-box-flex:2;
    width: 40%;       	
}
#wrapper-b,#wrapper-g{
    z-index: 4;
    left: 0;
    background: transparent;
    overflow: hidden;
    height:3.6rem;
}
.scroller {
    position: absolute;
    z-index: 4;
    -webkit-tap-highlight-color: rgba(0,0,0,0);
    width: 100%;
    -webkit-transform: translateZ(0);
    -moz-transform: translateZ(0);
    -ms-transform: translateZ(0);
    -o-transform: translateZ(0);
    transform: translateZ(0);
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    -webkit-text-size-adjust: none;
    -moz-text-size-adjust: none;
    -ms-text-size-adjust: none;
    -o-text-size-adjust: none;
    text-size-adjust: none;
    background-color: transparent;
}
.scroller ul {
    list-style: none;
    padding: 0;
    margin: 0;
    width: 100%;
    text-align: center;
}
.scroller li {
    padding: 0 0.2rem;
    height: 1.2rem;
    line-height: 1.2rem;
    background-color: #fafafa;
    font-size: 0.36rem;
    background-color: transparent
}

.cui-mask{
    position:absolute;
    z-index:10;
    top: 0;left: 0;
    width: 100%;
    height: 100%;
    pointer-events: none;
    background: linear-gradient(#fff 0,rgba(245,245,245,0) 52%,rgba(245,245,245,0) 48%,#fff 100%);
    /*background: -webkit-gradient(linear,left bottom,left top,from(#fff),color-stop(0.52,rgba(245,245,245,0)),color-stop(0.48,rgba(245,245,245,0)),to(#fff));*/
    background: -webkit-linear-gradient(#fff 0,rgba(245,245,245,0) 52%,rgba(245,245,245,0) 48%,#fff 100%);
    background: -moz-linear-gradient(#fff 0,rgba(245,245,245,0) 52%,rgba(245,245,245,0) 48%,#fff 100%);
}	
#logger{
    background: #d9d9d9;
    height: 100px;
    margin-bottom: 0.4rem;
    padding: 0.1rem;
    display: none;
}	
.li-img{
    width: 0.8rem;
    vertical-align: middle;
}
.li-span{
    line-height: 1.2rem;
    height: 1.2rem;
    vertical-align: middle;
    display: inline-block;
}

.activefnt-b{
    color: #078DD1;
}
.activefnt-g{
    color: #D0016E;
}
.arrow-left{
    display: inline-block;
    width: 0;
    height: 0;
    border-width:0.2rem;
    margin-left: -0.2rem;
    margin-right: 0.1rem;
    border-style: solid;
    border-color: #fff #D0016E #fff #fff; 
    vertical-align: middle;
}
.arrow-right{
    display: inline-block;
    width: 0;
    height: 0;
    border-width:0.2rem;
    margin-right:-0.2rem;
    margin-left: 0.1rem;
    border-style: solid;
    border-color: #fff #fff #fff #078DD1; 
    vertical-align: middle;
}

#reult-container{
    padding:0 0.2rem;
    display: none;
}			

.mt10{
    margin: 0.2rem 0;
    font-size: 0.32rem;
    line-height: 1.4;
    border-bottom: 1px dashed;
    padding-bottom: 0.1rem;
    color:#94a6ba;
    text-align:left;
}
.result-title{
    color: #fff;
    font-weight: bold;
    font-size: 0.32rem;
    background: #9bc53f;
    padding: 0.1rem 0.24rem;
    border-radius: 0.1rem;
    margin-right: 0.2rem;
    display: inline-block;
}
.mt10:nth-child(2) .result-title{
    background: #00adc1;  
}
.mt10:nth-child(3) .result-title{
    background: #e84c3d;  
}
.mt10:nth-child(4) .result-title{
    background: #f2a91c;  
}
.mt10:nth-child(5) .result-title{
    background: #f87c4b;
    margin-bottom: 0.2rem;
    display: inline-block;
}

.mt10:nth-child(6) .result-title{
    background: #f56f70;
    margin-bottom: 0.2rem;
    display: inline-block;   
}
                                                
.text-indent{
    text-indent:0.64rem;
    margin-top:0.2rem;
}

/* 导航 */	
header{position:fixed;top:0;left:0;width: 100%;}
.nav-wrap{
    border:none;
    height:0.9rem;
    background-color:#F56F70;
    display:-webkit-box;
    display: flex;
    width:100%;
    justify-content: center;
    -webkit-box-align:center;
    align-items: center;
    list-style:none
}
.nav-wrap li{-webkit-box-flex:1;border-right:1px solid #ffffff;flex: 1;}
.nav-wrap li:last-child{border:none}
.nav-a{
    display:block;
    text-decoration: none;
    line-height:0.7rem;
    height:0.7rem;
    font-size:0.32rem;
    color:#c00001;
    -webkit-tap-highlight-color: transparent;
}
.active{color:#ffffff;}